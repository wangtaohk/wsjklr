		Select Case current_step
		Case "审查"
			checkout= WebBrowser.RunJS(hWeb,'''function(){return document.querySelector('#btnSign').style.getPropertyValue('display')}''',True,{"bContinueOnError":False,"iDelayAfter":300,"iDelayBefore":200})
			// var display =document.querySelector('#btnSign').style.getPropertyValue('display');if(display == 'none'){return 2;}else{return 1;}
			If checkout=""
				each_lr_step_1()
			End If
			If checkout="none"
				each_lr_step_2()
			End If
		Case "现场核查"
			each_lr_step_3()
		Case "审批 "
			each_lr_step_4()   
		Case "发证"
			#icon("@res:bjlb9s4v-p4nv-fg92-giel-3vnc6sv5dgpm.png")
