//成功/失败数据写入文件
Function result_lr(id_lr,run_statue=0,result="录入成功")
	Dim file_time = Time.Format(Time.Date(),"yyyy-mm-dd")
	INI.Write(@res""&file_time&".ini", result, id_lr,run_statue )
End Function

